{
  "name": "infloww",
  "version": "5.6.0",
  "description": "Infloww for Agencies - OnlyFans Creator Management",
  "main": "electron/main.js",
  "author": "Infloww",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron . --dev\"",
    "electron:build": "vite build && electron-builder",
    "lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "antd": "^5.12.0",
    "recharts": "^2.10.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "eslint": "^8.55.0"
  },
  "build": {
    "appId": "com.infloww.app",
    "productName": "Infloww",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "src/assets/app-logo/logo.icns",
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": ["nsis", "portable"],
      "icon": "src/assets/app-logo/logo.png"
    }
  }
}
